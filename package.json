{
    "name": "vue-inline-text-editor",
    "version": "1.0.2",
    "description": "An inline text editor for VueJS.",
    "keywords": ["vue", "vuejs", "text editor", "component"],
    "license": "MIT",
    "main": "dist/vue-inline-text-editor.umd.js",
    "module": "dist/vue-inline-text-editor.esm.js",
    "unpkg": "dist/vue-inline-text-editor.min.js",
    "browser": {
        "./sfc": "src/vue-inline-text-editor.vue"
    },
    "scripts": {
        "build": "npm run build:umd & npm run build:es & npm run build:unpkg & npm run build:demo",
        "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-inline-text-editor.umd.js",
        "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-inline-text-editor.esm.js",
        "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-inline-text-editor.min.js",
        "build:demo": "cp -rf dist docs/dist",
        "test": "mocha-webpack --webpack-config conf/webpack.config.js --require test/setup.js test/**/*.spec.js",
        "lint": "eslint --ext .js,.vue src"
    },
    "dependencies": {
        "vue": "^2.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@vue/test-utils": "^1.0.0-beta.25",
        "babel-core": "^6.22.1",
        "babel-eslint": "^8.2.6",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-istanbul": "^5.0.1",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-plugin-transform-vue-jsx": "^3.5.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-stage-2": "^6.22.0",
        "chai": "^4.2.0",
        "cross-env": "^5.2.0",
        "css-loader": "^0.28.0",
        "eslint": "^5.7.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-html": "^4.0.6",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^4.7.1",
        "expect": "^23.6.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-webpack-plugin": "^2.30.1",
        "jsdom": "^12.2.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^5.2.0",
        "mocha-webpack": "^1.1.0",
        "nyc": "^13.0.1",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "regenerator-runtime": "^0.11.0",
        "rollup": "^0.57.1",
        "rollup-plugin-buble": "^0.19.2",
        "rollup-plugin-vue": "^3.0.0",
        "sass-loader": "^7.1.0",
        "vue-loader": "^12.0.0",
        "vue-template-compiler": "^2.1.0",
        "webpack": "^3.6.0",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dev-server": "^2.9.1",
        "webpack-merge": "^4.1.0",
        "webpack-node-externals": "^1.7.2"
    }
}
